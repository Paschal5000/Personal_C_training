#include<stdio.h>

// A program to check whether a year is a
// leap year or not

int main()
{
    int year;
    printf("Enter year:");
    scanf("%d", &year);
    
    if (year % 400 == 0 || year % 4 ==0)
    printf("Is a leap year");
    else if ( year % 100 == 0)
    printf("Not a leap year");
    else
    printf("Not a leap year");
    
    return 0;
}
