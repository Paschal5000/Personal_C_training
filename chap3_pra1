#include<stdio.h>

// A program to check whether a student
// passed or failed if it requires total 
// of 40% and at least 33% in each three subjects

int main()
{
int p = 40;
int a, b, c;
a = b = c = 33;
p = a+b+c;

// input marks for each three subjects
// arts, biology, chemistry

printf("Enter mark for arts:");
scanf("%d", &a);

printf("Enter mark for biology:");
scanf("%d", &b);

printf("Enter mark for chemistry:");
scanf("%d", &c);

printf("Enter total score:");
scanf("%d", &p);

if (a >= 33 || b >= 33 || c >= 33 || p >= 40)
printf("Congratulations, You Passed!");
else if (a < 33 || b < 33 || c < 33 || p < 40)
printf("You Failed!");

return 0;
}
